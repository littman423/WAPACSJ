	<queryString>
		<![CDATA[select ax.idEvaluacion,ax.idAlumno,u.nombre,u.apellidos,u.correo,ax.nota from 
AlumnoXEvaluacion ax inner join Alumno a 
on ax.idAlumno=a.idAlumno
inner join Usuario u 
on u.idUsuario = a.Usuario 
where ax.idEvaluacion = $P{p_idevaluacion} and ax.Seccion = $P{p_seccion} and ax.Curso =$P{p_curso} ;]]>
